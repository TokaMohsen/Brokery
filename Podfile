# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Brokery' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Brokery

   pod 'SDWebImage', '~> 4.0'
   pod 'Alamofire', '~> 4.2.0-rc.3'
   pod 'JSONModel' ,'~> 1.3.0'
   pod 'Reachability'
   pod 'UICKeyChainStore'
   pod 'Firebase/Analytics'
   pod 'Firebase/Auth'
   pod 'GoogleSignIn'

#pods for UI
   pod 'iOSDropDown'
   pod 'ReadMoreTextView'

#pods for google maps
  pod 'GoogleMaps'
  pod 'GooglePlaces'

# Pods for Facebook
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

  target 'BrokeryTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BrokeryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
