#pods for UI
   pod 'FSPagerView'
   pod 'iOSDropDown'

# Pods for Facebook
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

  target 'BrokeryTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BrokeryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

